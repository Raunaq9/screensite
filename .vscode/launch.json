  "version": "0.2.0",
  "configurations": [
    {
      "name": "SCREENSITE",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "toolArgs": [
        "--dart-define",
        "SANDBOX=true",
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "fetch",
      "program": "${workspaceFolder}/../screensrv/functions/lib/index.js",
      "runtimeArgs": [
        "--nolazy"
      ],
      "outFiles": [
        "${workspaceFolder}/../screensrv/lib/**/*.js"
      ],
      "sourceMaps": true
    },
  ]
}